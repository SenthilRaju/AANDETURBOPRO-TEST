	<queryString>
		<![CDATA[SELECT joRelease.ReleaseType,
cuInvoiceDetail.*, prMaster.ItemCode,
tsUserLogin.Initials AS SalesRep,
veShipVia.Description AS ShippedVia,
(CASE WHEN joMaster.Description  IS NULL THEN cuInvoice.jobnodescription ELSE joMaster.Description  END) AS Job,
cuInvoice.*,
cuInvoiceDetail.UnitCost*cuInvoiceDetail.QuantityBilled AS ExtList,
(IFNULL(NULLIF(cuInvoiceDetail.UnitCost, '' ), 0) * IFNULL(NULLIF(cuInvoiceDetail.QuantityBilled, '' ), 0)*IFNULL(NULLIF(cuInvoiceDetail.PriceMultiplier , '' ),1)) AS total,
tsUserLogin.Initials,cuTerms.Description AS termsdesc,
(SELECT headertext FROM tsUserSetting) AS header,
(SELECT companylogo FROM tsUserSetting) AS logo,
(SELECT remitToAddress1 FROM tsUserSetting) AS remitAddress1,
(SELECT remitToAddress2 FROM tsUserSetting) AS remitAddress2,
(SELECT remitToCity FROM tsUserSetting) AS remitToCity,
(SELECT remitToState FROM tsUserSetting) AS remitToState,
(SELECT remitToZip FROM tsUserSetting) AS remitToZip,
(SELECT remitToDescription FROM tsUserSetting) AS remitToDescription
FROM (joMaster RIGHT JOIN joRelease ON joMaster.joMasterID = joRelease.joMasterID LEFT JOIN joReleaseDetail ON joReleaseDetail.joReleaseID=joRelease.joReleaseID)
RIGHT JOIN
(veShipVia
RIGHT JOIN (cuInvoice LEFT JOIN tsUserLogin tsu ON cuInvoice.cuAssignmentID0 = tsu.UserLoginID)
ON veShipVia.veShipViaID = cuInvoice.veShipViaID) ON joReleaseDetail.joReleaseDetailID = cuInvoice.joReleaseDetailID
LEFT JOIN cuInvoiceDetail ON cuInvoiceDetail.cuInvoiceID = cuInvoice.cuInvoiceID
LEFT JOIN prMaster ON prMaster.prMasterID = cuInvoiceDetail.prMasterID
LEFT JOIN tsUserLogin ON tsUserLogin.UserLoginID = cuInvoice.cuAssignmentID0
LEFT JOIN cuTerms ON cuTerms.cuTermsID = cuInvoice.cuTermsID
WHERE cuInvoice.cuInvoiceID=$P{CuInvoice};]]>
	</queryString>
